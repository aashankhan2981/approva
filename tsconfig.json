, "components/forms/applicants/ContactInformationForm.tsx", "components/forms/applicants/ContactInformationForm.tsx", "components/icons/index.js", "components/util/LightToolTip.js", "src/pages/admin/index.js", "src/components/landing/home/sections/testimonial slider/testimonialtsx"  ],
  "exclude": [
    "node_modules"
  ]
}
